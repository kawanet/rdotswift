{
  "name": "rdotswift",
  "description": "iOS Swift code generator for Android app's res/values/strings.xml",
  "version": "1.0.4",
  "author": "@kawanet",
  "bin": {
    "rdotswift": "./rdotswift.cli.js"
  },
  "bugs": {
    "url": "https://github.com/kawanet/rdotswift/issues"
  },
  "dependencies": {
    "process.argv": "^0.6.0",
    "rdotjson": "^1.0.4"
  },
  "devDependencies": {
    "jshint": "^2.13.6",
    "mocha": "^10.2.0"
  },
  "homepage": "https://github.com/kawanet/rdotswift#readme",
  "jshintConfig": {
    "eqnull": true,
    "node": true,
    "undef": true,
    "unused": true
  },
  "files": [
    "*.js",
    "src"
  ],
  "keywords": [
    "Android",
    "Java",
    "R.string",
    "Swift",
    "XML",
    "iOS"
  ],
  "license": "MIT",
  "main": "./rdotswift.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/rdotswift.git"
  },
  "scripts": {
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint *.js *.json test/*.js src/*/*.js",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "prepack": "npm test",
    "test": "npm run jshint && npm run mocha"
  }
}
