{
  "name": "rdotswift",
  "description": "res/values/strings.xml to R.swift",
  "version": "1.0.0",
  "author": "@kawanet",
  "bin": {
    "rdotswift": "./rdotswift.cli.js"
  },
  "bugs": {
    "url": "https://github.com/kawanet/rdotswift/issues"
  },
  "dependencies": {
    "process.argv": "^0.1.0",
    "rdotjson": "^1.0.0"
  },
  "devDependencies": {
    "jshint": "^2.10.2",
    "mocha": "^6.1.4"
  },
  "homepage": "https://github.com/kawanet/rdotswift#readme",
  "jshintConfig": {
    "eqnull": true,
    "node": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "Android",
    "Java",
    "R",
    "Swift",
    "XML",
    "iOS",
    "strings.xml"
  ],
  "license": "MIT",
  "main": "./rdotswift.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/rdotswift.git"
  },
  "scripts": {
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint *.js *.json test/*.js src/*/*.js",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "test": "npm run jshint && npm run mocha"
  }
}
